import java.math.*;
public class Main
{
    
    static BigInteger factorial(int m){
        BigInteger f=new BigInteger("1");
        for(int i=2;i<=m;i++){
            f=f.multiply(BigInteger.valueOf(i));
        }
        return f;
    }
    
    
	public static void main(String[] args) {
		int n=3;
		int[] arr={10,11,13};
		for(int i=0;i<n;i++){
		    System.out.println(factorial(arr[i]));
		}
		
		
	}
}

